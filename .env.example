# Tinyproxy configuration
PROXY_PORT=8888
TIMEZONE=Europe/London

# Access control settings
ALLOWED_CLIENTS=*

# Proxy authentication
PROXY_USER=proxyuser
PROXY_PASSWORD=your_secure_password

# Proxy settings
ENABLE_FILTER=true
MAX_CLIENTS=100
MIN_SPARE_SERVERS=5
MAX_SPARE_SERVERS=20
START_SERVERS=10
MAX_REQUESTS_PER_CHILD=0